<div style='display: inline; float: right;'><image itemprop='image' class='filephoto' alt='Intrepid file photo [1474]' title='Intrepid file photo [1474]' src='/images/ship_intrepid1.jpg' /><script>
		function resizeText(className, percent) {
			var elements = document.getElementsByClassName(className);
			for (var i = 0; i < elements.length; ++i) {
				if (percent == null) {
					elements[i].style.fontSize = '100%';
				}
				else {
					if (elements[i].style.fontSize == '') elements[i].style.fontSize = '100%';
